extends ../base.pug 

block variables 
  - var template = 'home'

block content 
  .home 
    .home__wrapper
      .home__titles
        each item in items
          .offers__titles__title=item.data.title

      .home__gallery
        each media in home.data.gallery 
          figure.home__gallery__media
            img.home__gallery__image(alt=media.image.alt src=media.image.url)
      a(href="/offers")="Nasze Oferty"